{
	"manifest_version": 2,
	"name": "Anonymous Extension",
	"author": "kgsensei",
	"description": "A program to block risky websites and trackers.",
	"update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
	"default_locale": "en",
	"version": "1.3",
	"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js": ["jquery321.js"]
	}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "base_anon_home.html"
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"webNavigation",
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"<all_urls>"
	],
	"background": {
		"scripts": ["background.js"]
	}
}
